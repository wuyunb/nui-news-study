<view class="label"><view class="label-box"><view class="label-header"><view class="label-title">我的标签</view><view data-event-opts="{{[['tap',[['editClick',['$event']]]]]}}" class="label-edit" bindtap="__e">{{edit?'完成':'编辑'}}</view></view><block wx:if="{{labelList.length>0}}"><view class="label-content"><block wx:for="{{labelList}}" wx:for-item="item" wx:for-index="index" wx:key="_id"><view class="label-content_item">{{''+item.name+''}}<block wx:if="{{edit}}"><uni-icons class="closEnd" vue-id="{{'61899240-1-'+index}}" type="clear" size="15" color="red" data-event-opts="{{[['^click',[['dele',[index]]]]]}}" bind:click="__e" bind:__l="__l"></uni-icons></block></view></block></view></block><block wx:else><view class="no-data">暂无数据</view></block><view class="label-header"><view class="label-title">其他标签</view></view><block wx:if="{{list.length>0}}"><view class="label-content"><block wx:if="{{edit}}"><view class="label-content"><block wx:for="{{list}}" wx:for-item="item" wx:for-index="index" wx:key="_id"><view data-event-opts="{{[['tap',[['add',[index]]]]]}}" class="label-content_item" bindtap="__e">{{''+item.name+''}}</view></block></view></block><block wx:else><view class="label-content"><block wx:for="{{list}}" wx:for-item="item" wx:for-index="index" wx:key="_id"><view class="label-content_item">{{''+item.name+''}}</view></block></view></block></view></block><block wx:else><view class="no-data">暂无数据</view></block></view></view>